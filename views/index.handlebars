<main>
  <div class="container">
    <div class="row">
      <div class="col s12 intro animated slideInDown">
        STOP TEXTING AND JUST MEET.
      </div>
    </div>
    <!-- Modal Trigger -->
    <div class="centered">
      <a class="animated slideInDown waves-effect waves-light btn modal-trigger btn-large start-button"
        href="#modal1">Sign up</a>
    </div>

    <!-- Modal Structure -->
    <div id="modal1" class="modal">
      <form>
        <div class="modal-content">

          <div class="row">
            <div class="col s12 modalHeader">
              Get Started
            </div>
          </div>
          {{!-- gender --}}
          <div class="row">
            <div class="input-field col s4">
              <select id="gender" class="validate" required="required">
                <option value="" disabled selected>Gender</option>
                <option value="M">Male </option>
                <option value="F">Female</option>
              </select>
              <label>Your Gender</label>
            </div>
            {{!--  --}}
            <div class="input-field col s4">
              <select id="genderPreference" class="validate" required="required">
                <option value="" disabled selected>Gender</option>
                <option value="M">Male</option>
                <option value="F">Female </option>
              </select>
              <label>Preference</label>
            </div>
            {{!--  --}}
            {{!-- age --}}
            <div class="col s4">
              <label>Select your age.</label>
              <select class="browser-default validate" id="age" required="required">
                <option value="" disabled selected> Age</option>
                <option value="18">18</option>
                <option value="19">19</option>
                <option value="20">20</option>
                <option value="21">21</option>
                <option value="22">22</option>
                <option value="23">23</option>
                <option value="24">24</option>
                <option value="25">25</option>
                <option value="26">26</option>
                <option value="27">27</option>
                <option value="28">28</option>
                <option value="29">29</option>
                <option value="30">30</option>
                <option value="31">31</option>
                <option value="32">32</option>
                <option value="33">33</option>
                <option value="34">34</option>
                <option value="35">35</option>
                <option value="36">36</option>
                <option value="37">37</option>
                <option value="38">38</option>
                <option value="39">39</option>
                <option value="40">40</option>
                <option value="41">41</option>
                <option value="42">42</option>
                <option value="43">43</option>
                <option value="44">44</option>
                <option value="45">45</option>
                <option value="46">46</option>
                <option value="46">47</option>
                <option value="46">48</option>
                <option value="46">49</option>
                <option value="46">50</option>
                <option value="46">51</option>
                <option value="46">52</option>
                <option value="46">53</option>
                <option value="46">54</option>
                <option value="46">55</option>
                <option value="46">56</option>
                <option value="46">57</option>
                <option value="46">58</option>
                <option value="46">59</option>
                <option value="46">60</option>
              </select>
            </div>
          </div>
          {{!--  --}}
          {{!-- name --}}
          <div class="col s12">
            <div class="row">
              <div class="input-field col s6">
                <i class="material-icons prefix">account_circle</i>
                <input id="first_name" type="text" name="username" required="required">
                <label for="first_name">First Name</label>
              </div>
              <div class="input-field col s6">
                <input id="last_name" type="text" class="validate" required="required">
                <label for="last_name">Last Name</label>
              </div>
            </div>
            {{!--  --}}
            {{!-- email --}}
            <div class="row">
              <div class="input-field col s6">
                <input id="email" type="email" class="validate" required="required">
                <label for="email">Email</label>
              </div>
              <div class="input-field col s6">
                <input id="password" type="password" class="validate" required="required">
                <label for="password">Password</label>
              </div>
            </div>
            {{!--  --}}
            <div class="row">
              {{!-- zipcode --}}
              <div class="col s3">
                <div class="input-field">
                  <input id="zipCode" type="text" maxlength="5">
                  <label for="input_text">Zip code</label>
                </div>
              </div>
              {{!--  --}}
            </div>
            {{!-- <div class="modal-footer"> --}}
            <div class="row">
              <div class="col s8">
                <fb onlogin="checkLoginState();">
                  <div class="fb-login-button" data-scope="public_profile,user_link" data-size="large"
                    data-button-type="continue_with" data-auto-logout-link="true" data-use-continue-as="false"></div>
                </fb>
                <div class="row">
                  <div class="col s12">We'll NEVER post anything. Ever.</div>
                </div>
              </div>
              <div class="col4">
                <button class="btn waves-effect waves-light submitBtn" id="btnSubmit" type="submit" name="action">Submit
                </button>
              </div>
              <div id="status">
              </div>
            </div>
          </div>
      </form>
    </div>
  </div>
  <!-- Modal Structure -->
  <div id="modal2" class="modal">
    <div class="modal-content">
      <iframe class="i-frame" src="http://www.mcflysoftware.com/eros-privacy-policy.html"></iframe>
    </div>
    <div class="modal-footer">
      <a href="#!" class="modal-close waves-effect waves-green btn-flat">Agree</a>
    </div>
  </div>
